<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.2//EN" "http://struts.apache.org/dtds/xwork/xwork-validator-1.0.2.dtd">
<validators>

	<field name="uname">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message key="username.required">用户名为空？</message>
		</field-validator>
	</field>
	
	<field name="password">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message key="password.required"></message>
		</field-validator>
	</field>

</validators>